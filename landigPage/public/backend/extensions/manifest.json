{
  "manifest_version": 2,
  "name": "Baixar CSV",
  "version": "1.0",
  "description": "Baixe a planilha do banco de dados aqui.",
  "permissions": [
    "activeTab",
    "storage",
    "https://*/*",
    "http://*/*",
    "downloads"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "128": "icon.png"
  }
}
